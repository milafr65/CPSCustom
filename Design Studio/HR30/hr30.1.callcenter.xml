		<XSCRIPT id="script1" nbr="_s1">
			<![CDATA[
function FORM_OnAfterDataInit()
{
   tranMagic.clearFormData();
}
	
function FORM_OnAfterTransaction(data)
{
      
   // Trap for errors on HR30 screen
      
      var errMsgNbr = lawForm.getMessageNbr();
      var errMsg    = lawForm.getMessage();
      if (errMsgNbr != "000")
      { 
      //   portalWnd.cmnDlg.messageBox(errMsg,"ok","alert",window);
         return; 
      }
      
   // tfrKEY will contain the values to pass from HR30 to the transfer screen
      var tfrKEY="";

   // Retrieve function code
      var fc=data.selectSingleNode("//_f1").text;

   // From trial and error, it appears HCP_DATA_REQUESTED has to be retrieved
   // from an element value instead of a form value because the field is not 
   // always displayed on-screen. In testing, when retrieving the form value, the
   // form transfer only worked when the Request tab was active, displaying
   // the field. Retrieving the element value appears to work no matter which
   // tab is active. To be consistent, all of the Lawson fields will be retrieved
   // as element values.

   // HRCORRESP.HCP_DATA_REQUESTED = element value _f46
   // HRCORRESP.HCP_REQ_TYPE = element value _f5, form value select1
   // HRCORRESP.HCP_COMPANY = element value _f3, form value text1
   // HRCORRESP.HCP_ID_NBR = element value _f8, form value text2
   // HRCORRESP.HCP_DEPENDENT = element value _f10, form value text3

      if (fc == "A")
   {
      //portalWnd.cmnDlg.messageBox("Initiating add","ok","alert",window);
         
      var HCP_DATA_REQUESTED=lawForm.getElementValue("_f46");
      var HCP_COMPANY=lawForm.getElementValue("_f3");
      var HCP_REQ_TYPE=lawForm.getElementValue("_f5");
      var HCP_ID_NBR=lawForm.getElementValue("_f8");
      var HCP_DEPENDENT=lawForm.getElementValue("_f10");

      // if DATA_REQUESTED is populated using the drop-down menu,
      // it returns with a length of 10 with trailing spaces
      // JS does not have a function to chomp trailing spaces
      // This code removes leading and trailing spaces using regular expressions
  
      HCP_DATA_REQUESTED = HCP_DATA_REQUESTED.replace(/^\s+|\s+$/g, "");

      // tfrID_NBR is the id-nbr padded with lead zeroes to nine digits
      // seems to be needed for the PA52 form transfer
      // ZH11/HR13 don't appear to need the lead zeroes but will
      // send that way anyway.
     
      var tfrID_NBR = "";
      var z = 0;

      z = 9 - HCP_ID_NBR.length;

      for (i=1; i<=z; i++)
      {
        tfrID_NBR += "0";
      }

      tfrID_NBR += HCP_ID_NBR;

      // tfrDate is a yyyymmdd string
      // that can be passed in the PA52 form transfer

      // Retrieve 4-digit year
      var myDate = new Date();
      var myYear = myDate.getFullYear();
      
      // month values in JS are 0 thru 11
      var myMonth = myDate.getMonth() + 1;
      var myDay = myDate.getDate();
   
      // field concatenation
      var tfrDate = "";
      tfrDate += myYear;

       if (myMonth < 10)
      { 
         tfrDate += "0";
      }
  
      tfrDate += myMonth;

     if (myDay < 10)
     { 
        tfrDate += "0";
     }
     
    tfrDate += myDay;

    // Tests for form transfer conditions

    // For address changes, do not invoke screen transfer for dependents
    // CPS doesn't do any dependent address changes here
     
      if (HCP_DATA_REQUESTED == "ADDRESSCHG")
      {
       if (HCP_REQ_TYPE != "4")
         {
         tfrKEY = HCP_COMPANY + tfrID_NBR;
         lawForm.pushFormWindow ("ZH11",strPDL,tfrKEY);
         }
      }

      if (HCP_DATA_REQUESTED == "EMAIL")
      {
         tfrKEY = HCP_COMPANY + tfrID_NBR;
         lawForm.pushFormWindow ("ZHEM.1",strPDL,tfrKEY);
       }
 
      if  (HCP_DATA_REQUESTED == "SSN")
      {      
       if (HCP_REQ_TYPE == "1")
           {
           tfrKEY = HCP_COMPANY + tfrID_NBR + "PERSDATACH" + tfrDate;
           lawForm.pushFormWindow ("PA52.1",strPDL,tfrKEY);
           }
       if (HCP_REQ_TYPE == "4")
          {
           tfrKEY = HCP_COMPANY + tfrID_NBR + HCP_DEPENDENT;
           lawForm.pushFormWindow ("HR13",strPDL,tfrKEY);
          }
      }

      if  (HCP_DATA_REQUESTED == "PHONE")
      {      
       if (HCP_REQ_TYPE == "1")
           {
           tfrKEY = HCP_COMPANY + tfrID_NBR + "PHONE CHG " + tfrDate;
           lawForm.pushFormWindow ("PA52.1",strPDL,tfrKEY);
           }
       if (HCP_REQ_TYPE == "4")
          {
           tfrKEY = HCP_COMPANY + tfrID_NBR + HCP_DEPENDENT;
           lawForm.pushFormWindow ("HR13",strPDL,tfrKEY);
          }
      }

   }
     // return false would inhibit the HR30 transaction
     return true;	
}

]]>
		</XSCRIPT>
