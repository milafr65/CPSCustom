P58566******************************************************************
P58566 2200-UPDATE-PRSYSTEM            SECTION 50.
P58566******************************************************************
P58566 2200-START.
P58566
P58566     MOVE WS-MAX-OPS-IN-TRAN     TO HR511WS-MAX-OPS-IN-TRAN.
P58566     MOVE PRM-COMPANY            TO DB-COMPANY.
P58566     MOVE SPACES                 TO DB-PROCESS-LEVEL.
P58566     PERFORM 840-MODIFY-PRSSET1.
P58566     IF (PRS-LAST-EMPLOYEE = ZEROES)
P58566         MOVE HR511WS-MAX-OPS-IN-TRAN   
P58566                                 TO PRS-LAST-EMPLOYEE
P58566         PERFORM 820-STORE-PRSYSTEM
P58566         MOVE 1                  TO HR511WS-EMPLOYEE
P58566                                    HR511WS-FIRST-EMP
P58566         GO TO 2200-END.
P58566
P58566     MOVE PRS-LAST-EMPLOYEE      TO DB-EMPLOYEE.
P58566     PERFORM 840-KFIND-EMPSET1.
P58566     IF (EMPLOYEE-KNOTFOUND)
P58566         PERFORM
P58566             UNTIL (EMPLOYEE-KNOTFOUND) 
P58566             IF (EMP-EMPLOYEE = 999999999)
P58566                 MOVE 1          TO DB-EMPLOYEE
P58566             ELSE
P58566                 ADD 1           TO DB-EMPLOYEE
P58566             END-IF
P58566             PERFORM 840-KFIND-EMPSET1
P58566         END-PERFORM.
P58566
P58566     MOVE DB-EMPLOYEE            TO HR511WS-FIRST-EMP.
P58566     PERFORM 850-FIND-NLT-EMPSET1.
P58566     PERFORM 860-FIND-NEXT-EMPSET1.
P58566     IF (EMPLOYEE-NOTFOUND)
P58566     OR (EMP-COMPANY NOT = DB-COMPANY)
P58566         COMPUTE PRS-LAST-EMPLOYEE =
P58566             (HR511WS-FIRST-EMP + HR511WS-MAX-OPS-IN-TRAN)
P58566         COMPUTE HR511WS-EMPLOYEE = (HR511WS-FIRST-EMP + 1)
P58566     ELSE
P58566         MOVE EMP-EMPLOYEE       TO HR511WS-NEXT-EMP
P58566         COMPUTE HR511WS-TOTAL-EMPS =
P58566             (HR511WS-NEXT-EMP - HR511WS-FIRST-EMP)
P58566         IF (HR511WS-TOTAL-EMPS < HR511WS-MAX-OPS-IN-TRAN)
P58566             COMPUTE PRS-LAST-EMPLOYEE = 
P58566               (HR511WS-NEXT-EMP - 1)
P58566             COMPUTE HR511WS-MAX-OPS-IN-TRAN =
P58566               (HR511WS-NEXT-EMP - HR511WS-FIRST-EMP)
P58566             COMPUTE HR511WS-EMPLOYEE = (HR511WS-FIRST-EMP + 1)
P58566         ELSE
P58566             COMPUTE PRS-LAST-EMPLOYEE =
P58566               (HR511WS-FIRST-EMP + HR511WS-MAX-OPS-IN-TRAN)
P58566             COMPUTE HR511WS-EMPLOYEE = (HR511WS-FIRST-EMP + 1)
P58566         END-IF
P58566     END-IF.
P58566
P58566     PERFORM 820-STORE-PRSYSTEM.
P58566
P58566     PERFORM 925-AUDIT-END.
P58566     PERFORM 910-AUDIT-BEGIN.
P58566
P58566******************************************************************
P58566 2200-END.
P58566******************************************************************
      ******************************************************************
       3000-PRINT-PXL                  SECTION 50.
      ******************************************************************

           IF  (PXL-NO-UPDATES)
               GO TO 3000-END.


           MOVE HREMP-EMPLOYEE         TO R2G2-EMP-EMPLOYEE.
           MOVE HREMP-LAST-NAME        TO HRWS-LAST-NAME.
           MOVE EMP-FIRST-NAME         TO HRWS-FIRST-NAME.
           MOVE EMP-MIDDLE-INIT        TO HRWS-MIDDLE-INIT.
           MOVE EMP-LAST-NAME-PRE      TO HRWS-LAST-NAME-PRE.
           MOVE EMP-NAME-SUFFIX        TO HRWS-NAME-SUFFIX.
           PERFORM 750-HR-FORMAT-NAME.
           MOVE HRWS-FORMAT-NAME       TO R2G2-EMP-NAME.
           MOVE R2GN2-EMPLOYEE         TO RPT-GROUP-REQUEST.
           PERFORM 700-PRINT-RPT-GRP.

           INITIALIZE PRPXL-LOCAT-CODE.
           PERFORM
               VARYING I1 FROM 1 BY 1
               UNTIL  (I1 > PRPXL-TAX-CNT)
               IF  (NOT PXL-NOUPDATE (I1))
                   PERFORM 3003-CLEAR-LINE
                   THRU    3003-END
                   PERFORM 3004-TAX-INFO
                   THRU    3004-END
                   PERFORM
                       VARYING I2 FROM 1 BY 1
                       UNTIL  (I2 > PRPXL-EDM-CNT)
                       OR     (PRPXL-EDM-DED-CODE (I2) =
                                               PRPXL-TAX-DEDUCTION (I1))
                       CONTINUE
                   END-PERFORM
                   IF  (I2 <= PRPXL-EDM-CNT)
                       PERFORM 3005-EDM-INFO
                       THRU    3005-END
                   END-IF
                   MOVE R2GN3-PXL-DETAIL       TO RPT-GROUP-REQUEST
                   PERFORM 700-PRINT-RPT-GRP
               END-IF
           END-PERFORM.

           PERFORM
               VARYING I2 FROM 1 BY 1
               UNTIL  (I2 > PRPXL-EDM-CNT)
               IF  (NOT EDM-NOUPDATE (I2))
               AND (NOT EDM-PRINTED (I2))
                   PERFORM 3003-CLEAR-LINE
                   THRU    3003-END
                   PERFORM 3005-EDM-INFO
                   THRU    3005-END
                   MOVE R2GN3-PXL-DETAIL       TO RPT-GROUP-REQUEST
                   PERFORM 700-PRINT-RPT-GRP
               END-IF
           END-PERFORM.

           GO TO 3000-END.

      *****************************************************************
       3003-CLEAR-LINE.

           INITIALIZE     R2G3-HOME-LOCATION
                          R2G3-TR-LOCATION
                          R2G3-SOURCE
                          R2G3-DED-CODE
                          R2G3-LOC-MAINT
                          R2G3-DED-MAINT
                          R2G3-EFFECT-DATE
                          R2G3-END-DATE
                          R2G3-RES-CODE.

       3003-END.

      *****************************************************************
       3004-TAX-INFO.

           IF  (PRPXL-TAX-LOCATION (I1) NOT = PRPXL-LOCAT-CODE)
               IF  (PRIMARY-LOCATION (I1))
                   MOVE PRPXL-TAX-LOCATION (I1)    TO R2G3-HOME-LOCATION
               ELSE
                   MOVE PRPXL-TAX-LOCATION (I1)    TO R2G3-TR-LOCATION
               END-IF
           END-IF.
           MOVE PRPXL-TAX-LOCATION (I1)    TO PRPXL-LOCAT-CODE.
           IF  (PXL-SYSTEM-REC (I1))
               MOVE "S"                            TO R2G3-SOURCE.
           IF  (PXL-USER-REC (I1))
               MOVE "U"                            TO R2G3-SOURCE.
           IF  (PXL-DISABLED (I1))
               MOVE "D"                            TO R2G3-SOURCE.
           IF  (PXL-M-ADD (I1))
               MOVE "A"                            TO R2G3-LOC-MAINT.
           IF  (PXL-M-DELETE (I1))
               MOVE "D"                            TO R2G3-LOC-MAINT.
           IF  (PXL-M-INACT (I1))
               MOVE "I"                            TO R2G3-LOC-MAINT.
           IF  (PXL-M-REACT (I1))
               MOVE "R"                            TO R2G3-LOC-MAINT.
           IF  (PXL-M-CHANGE (I1))
               MOVE "C"                            TO R2G3-LOC-MAINT.
           MOVE PRPXL-TAX-DEDUCTION (I1)           TO R2G3-DED-CODE.

       3004-END.

      *****************************************************************
       3005-EDM-INFO.

           MOVE PRPXL-EDM-DED-CODE (I2)    TO R2G3-DED-CODE.
           IF  (EDM-M-ADD (I2))
               MOVE "A"                    TO R2G3-DED-MAINT.
           IF  (EDM-M-END (I2))
               MOVE "E"                    TO R2G3-DED-MAINT.
           IF  (EDM-M-REACT (I2))
               MOVE "R"                    TO R2G3-DED-MAINT.
           IF  (EDM-M-CHANGE (I2))
               MOVE "C"                    TO R2G3-DED-MAINT.
           IF  (EDM-M-ADD (I2))
           OR  (EDM-M-REACT (I2))
               MOVE PRPXL-EFFECT-DATE      TO R2G3-EFFECT-DATE.
           IF  (EDM-M-END (I2))
               MOVE PRPXL-END-DATE         TO R2G3-END-DATE.
           MOVE PRPXL-EDM-RES-CODE (I2)    TO R2G3-RES-CODE.
           SET EDM-PRINTED (I2) TO TRUE.

       3005-END.

      ******************************************************************
       3000-END.
