023300     MOVE CVL-EMP-COMPANY        TO HREMP-COMPANY.
023400     MOVE 1                      TO HREMP-COMPANY-FN.
023500     MOVE CVL-EMP-EMPLOYEE       TO HREMP-EMPLOYEE.
023600     MOVE 1                      TO HREMP-EMPLOYEE-FN.
023700
           IF  ((CVL-EMP-FC             = "C")
023800     AND  (CVL-EMP-LAST-NAME-PRE  = SPACES))
           OR  (CVL-PEP-POS-LEVEL        > 1)
023900         MOVE EMP-LAST-NAME-PRE  TO CVL-EMP-LAST-NAME-PRE.
024000
           IF  ((CVL-EMP-FC        = "C")
023800     AND  (CVL-EMP-LAST-NAME = SPACES))
           OR  (CVL-PEP-POS-LEVEL        > 1)
023900         MOVE EMP-LAST-NAME    TO CVL-EMP-LAST-NAME.
024000
           IF  ((CVL-EMP-FC         = "C")
024100     AND  (CVL-EMP-FIRST-NAME = SPACES))
           OR  (CVL-PEP-POS-LEVEL        > 1)
024200         MOVE EMP-FIRST-NAME   TO CVL-EMP-FIRST-NAME.
024300
           IF  ((CVL-EMP-FC          = "C")
024400     AND  (CVL-EMP-MIDDLE-INIT = SPACES))
           OR  (CVL-PEP-POS-LEVEL        > 1)
024500         MOVE EMP-MIDDLE-INIT  TO CVL-EMP-MIDDLE-INIT.
024600
065301     IF (CVL-EMP-MIDDLE-INIT       = SPACES)
065301     AND (CVL-EMP-MIDDLE-NAME  NOT = SPACES)
065301         MOVE CVL-EMP-MIDDLE-NAME (1:1)
065301                                  TO CVL-EMP-MIDDLE-INIT
065301     END-IF.
065301     MOVE 1                       TO HRCASE-SIZE.
065301     MOVE CVL-EMP-MIDDLE-INIT     TO HRCASE-UP-FIELD.
065301     PERFORM 2500-HR-UPPER-CASE.
065301     MOVE HRCASE-UP-FIELD         TO CVL-EMP-MIDDLE-INIT.
065301
065301     MOVE CVL-EMP-LAST-NAME       TO G1B-LAST-NAME.
065301     MOVE CVL-EMP-FIRST-NAME      TO G1B-FIRST-NAME.
065301     MOVE CVL-EMP-MIDDLE-INIT     TO G1B-MIDDLE-INIT.
025000
           INITIALIZE                       HREMP-UPDPEP-DATE
                                            PAPEP-UPDPEP-DATE
                                            PADT-UPDPEP-DATE.

           MOVE CVL-EMP-COMPANY          TO PADT-COMPANY.
           MOVE CVL-EMP-EMPLOYEE         TO PADT-EMPLOYEE.
           MOVE CVL-PEP-POS-LEVEL        TO PADT-POS-LEVEL.
           MOVE CVL-PEP-EFFECT-DATE      TO PADT-EFFECT-DATE.
           IF  (CVL-EMP-FC        = "A")
           AND (CVL-PEP-POS-LEVEL = 1)
               IF (WS-CVL-EMP-TERM-DATE-A NUMERIC)
                   MOVE CVL-EMP-TERM-DATE
                                         TO PADT-END-DATE
               ELSE
                   INITIALIZE               PADT-END-DATE
               END-IF
           ELSE
           IF  (CVL-EMP-FC        = "A")
           AND (CVL-PEP-POS-LEVEL > 1)
               MOVE EMP-TERM-DATE      TO PADT-END-DATE
           END-IF.

           PERFORM 2300-PADT-DATE-CHECK.
           IF (ERROR-FOUND)
               SET EMP-ERROR-FOUND TO TRUE
               MOVE CRT-ERROR-NBR      TO CRT-MSG-NBR
               PERFORM 790-GET-MSG
               MOVE CRT-MESSAGE        TO G2-ERROR-MESSAGE
               MOVE GN2-ERROR-LINE     TO RPT-GROUP-REQUEST
               PERFORM 700-PRINT-RPT-GRP
               GO TO 1200-END.

622700     IF (PADT-UPDPEP-DATE NOT = ZEROES)
               MOVE PADT-UPDPEP-DATE   TO PAPEP-UPDPEP-DATE
                                          HREMP-UPDPEP-DATE.

           IF (CVL-PEP-POS-LEVEL        > 1)
               PERFORM 1220-MOVE-TO-PAPEP
               THRU    1220-END
               IF (EMP-ERROR-FOUND)
                   GO TO 1200-END
               END-IF
               IF  (PAEMPPOS-FOUND)
               AND (PEP-COMPANY   = DB-COMPANY)
               AND (PEP-EMPLOYEE  = DB-EMPLOYEE)
               AND (PEP-POS-LEVEL = DB-POS-LEVEL)
                   PERFORM 7000-PAPEP-DEFAULT
               END-IF
           END-IF.

           IF (CVL-PEP-POS-LEVEL        > 1)
               IF (CVL-PEP-EFFECT-DATE = PADT-PEP-DATE)
                   MOVE "C"            TO PAPEP-FC
               ELSE
               IF (PADT-UPDPEP-DATE NOT = ZEROES)
                   MOVE "C"            TO PAPEP-FC
               ELSE
                   MOVE "A"            TO PAPEP-FC
               END-IF
               END-IF
               PERFORM 2000-PAPEP-EDIT-TRAN
               IF  (HREMP-ERROR-FOUND)
               OR  (ERROR-FOUND)
                   SET EMP-ERROR-FOUND TO TRUE
               END-IF
               IF  (HREMP-ERROR-FOUND)
               OR  (HREMP-WARNING-FOUND)
               OR  (ERROR-FOUND)
                   PERFORM 1205-PROCESS-MESSAGES
                   THRU    1205-END
               END-IF
               GO TO 1200-END.

025600     PERFORM 2000-HREMP-EDIT-TRAN.

           IF  (CVL-PEP-POS-LEVEL    = 1)
           AND (CVL-PEP-END-DATE     NOT = ZEROES)
           AND (HREMP-TERM-DATE      = ZEROES)
020500         SET EMP-ERROR-FOUND      TO TRUE
               MOVE "HR511"             TO CRT-ERROR-CAT
020700         MOVE 105                 TO CRT-MSG-NBR
020800         PERFORM 790-GET-MSG
021000         PERFORM 1300-PRINT-ERROR
021100         THRU    1300-END
               GO TO 1200-END.

           IF  (HREMP-ERROR-FOUND)
           OR  (ERROR-FOUND)
               SET EMP-ERROR-FOUND TO TRUE
           END-IF.

026100     IF  (HREMP-ERROR-FOUND)
           OR  (HREMP-WARNING-FOUND)
           OR  (ERROR-FOUND)
               PERFORM 1205-PROCESS-MESSAGES
               THRU    1205-END
           END-IF.

           IF  (TAX-LOCATOR)
           AND (NOT PRPXL-UPDATES-BYPASS)
               PERFORM 3000-PRINT-PXL.

025800     IF  (EMP-ERROR-FOUND)
025900         GO TO 1200-END.
027900
033700 1200-END.

      ******************************************************************
       1205-PROCESS-MESSAGES.
      ******************************************************************

           IF  (HREMP-I2 NOT = ZEROS)
               IF  (HREMP-ERROR-FOUND)
                   SET EMP-ERROR-FOUND TO TRUE
               END-IF
               PERFORM
                   VARYING I1 FROM 1 BY 1
                   UNTIL  (I1 > HREMP-I2)

                   INITIALIZE WS-FIELD-VALUE
                   PERFORM 1280-GET-FIELD-VALUE
                   THRU    1280-END
J20855*            IF (WS-FIELD-VALUE  = SPACES)
J20855*                MOVE HREMP-MESSAGE (I1) TO G2-ERROR-MESSAGE
J20855*            ELSE
J20855*                MOVE HREMP-MESSAGE (I1) TO WS-ERR-MSG
J20855*                MOVE WS-ERROR-MESSAGE   TO G2-ERROR-MESSAGE
J20855*            END-IF
J20855             MOVE HREMP-MESSAGE (I1)     TO G2-ERROR-MESSAGE
J20855             MOVE WS-FIELD-VALUE         TO G2-FIELD-VALUE
                   MOVE GN2-ERROR-LINE         TO RPT-GROUP-REQUEST
                   PERFORM 700-PRINT-RPT-GRP
               END-PERFORM
           ELSE
               MOVE CRT-ERROR-NBR          TO CRT-MSG-NBR
               PERFORM 790-GET-MSG
               MOVE CRT-MESSAGE            TO G2-ERROR-MESSAGE
               MOVE GN2-ERROR-LINE         TO RPT-GROUP-REQUEST
               PERFORM 700-PRINT-RPT-GRP
               GO TO 1205-END
           END-IF.

           IF  (HREMP-NO-ERROR-FOUND)
               MOVE 300                    TO CRT-MSG-NBR
               PERFORM 790-GET-MSG     
               MOVE CRT-MESSAGE            TO G2-ERROR-MESSAGE
               MOVE GN2-ERROR-LINE         TO RPT-GROUP-REQUEST
               PERFORM 700-PRINT-RPT-GRP
           END-IF.

       1205-END.

