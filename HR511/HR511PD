               INITIALIZE                          DB-APPLICANT
               PERFORM 850-FIND-NLT-PPPSET2
               IF  (PAEMPAPP-FOUND)
               AND (PPP-COMPANY  = DB-COMPANY)
               AND (PPP-EMPLOYEE = DB-EMPLOYEE)
                   MOVE PPP-APPLICANT           TO DB-APPLICANT
                   PERFORM 840-MODIFY-PPPSET1
                   PERFORM 830-DELETE-PAEMPAPP.

           IF (CVL-PPP-APPLICANT NOT = ZEROES)
               IF (CVL-EMP-FC = "A")
                   PERFORM 800-CREATE-PAEMPAPP
                   MOVE HREMP-COMPANY          TO PPP-COMPANY
                   MOVE HREMP-EMPLOYEE         TO PPP-EMPLOYEE
                   MOVE CVL-PPP-APPLICANT      TO PPP-APPLICANT
                   PERFORM 820-STORE-PAEMPAPP
               ELSE
               IF (CVL-EMP-FC = "C")
                   MOVE HREMP-COMPANY          TO DB-COMPANY
                   MOVE HREMP-EMPLOYEE         TO DB-EMPLOYEE
                   INITIALIZE                     DB-APPLICANT
                   PERFORM 850-FIND-NLT-PPPSET2
                   IF  (PAEMPAPP-FOUND)
                   AND (PPP-COMPANY  = DB-COMPANY)
                   AND (PPP-EMPLOYEE = DB-EMPLOYEE)
                       IF (PPP-APPLICANT NOT = CVL-PPP-APPLICANT)
                           MOVE PPP-APPLICANT  TO DB-APPLICANT
                           PERFORM 840-MODIFY-PPPSET1
                           PERFORM 830-DELETE-PAEMPAPP
                           PERFORM 800-CREATE-PAEMPAPP
                           MOVE HREMP-COMPANY  TO PPP-COMPANY
                           MOVE HREMP-EMPLOYEE TO PPP-EMPLOYEE
                           MOVE CVL-PPP-APPLICANT
                                               TO PPP-APPLICANT
                           PERFORM 820-STORE-PAEMPAPP
                       END-IF
                   ELSE
                       PERFORM 800-CREATE-PAEMPAPP
                       MOVE HREMP-COMPANY      TO PPP-COMPANY
                       MOVE HREMP-EMPLOYEE     TO PPP-EMPLOYEE
                       MOVE CVL-PPP-APPLICANT  TO PPP-APPLICANT
                       PERFORM 820-STORE-PAEMPAPP.

P64863     IF (HREMP-POS-LEVEL         = 1)
P64863         IF (CVL-PEM-NEXT-REV-CODE NOT = SPACES)
P64863         OR (CVL-PEM-NEXT-REVIEW   NOT = ZEROES)
P64863             MOVE HREMP-COMPANY              TO DB-COMPANY
P64863             MOVE HREMP-EMPLOYEE             TO DB-EMPLOYEE
P64863             PERFORM 840-MODIFY-PEMSET1
P64863         END-IF.
P64863
P64863     IF  (HREMP-POS-LEVEL         = 1)
           AND (CVL-PEM-NEXT-REV-CODE   NOT = SPACES)
           AND (CVL-PEM-NEXT-REVIEW     NOT = ZEROES)
               PERFORM 800-CREATE-REVIEW
               INITIALIZE                         REVIEW
               MOVE HREMP-COMPANY              TO REV-COMPANY
               MOVE HREMP-EMPLOYEE             TO REV-EMPLOYEE
               COMPUTE REV-SEQ-NBR = (PEM-LAST-REV-SEQ + 1)
               MOVE CVL-PEM-NEXT-REVIEW        TO REV-SCHED-DATE
               MOVE CVL-PEM-NEXT-REV-CODE      TO REV-CODE
               MOVE CVL-PEM-LAST-REVIEW        TO REV-LAST-REVIEW
               MOVE WS-SYSTEM-DATE-YMD         TO REV-DATE-STAMP
               MOVE HHMMSS                     TO REV-TIME-STAMP
               MOVE CRT-USER-NAME              TO REV-USER-ID
               PERFORM 820-STORE-REVIEW
P64863         MOVE REV-SEQ-NBR                TO PEM-LAST-REV-SEQ.
P64863
P64863     IF (HREMP-POS-LEVEL         = 1)
P64863         IF (CVL-PEM-NEXT-REV-CODE   NOT = SPACES)
P64863         OR (CVL-PEM-NEXT-REVIEW     NOT = ZEROES)
                   MOVE CVL-PEM-NEXT-REVIEW    TO PEM-NEXT-REVIEW
                   MOVE CVL-PEM-NEXT-REV-CODE  TO PEM-NEXT-REV-CODE
                   PERFORM 820-STORE-PAEMPLOYEE.

P58566     IF (PRM-MASS-LOAD = "N")
